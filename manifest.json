{
  "manifest_version": 2,
  "name": "<Table> Analytics",
  "version": "0.1",
  "background": {
    "scripts": [
      "./background/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "./src/analytics.css"
      ],
      "js": [
        "./content/jquery-3.1.0.min.js",
        "./content/contentScript.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "./src/index.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}